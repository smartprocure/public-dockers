{
    "server": {
        "accessLog": "/var/log/nginx/access.log",
        "workers": 10,
        "maxSockets": 100,
        "deadBackendTTL": 30,
        "tcpTimeout": 30,
        "retryOnError": 3,
        "deadBackendOn500": true,
        "httpKeepAlive": false
    },
    "https": {
        "port": 443,
        "bind": "0.0.0.0",
        "key": "/etc/ssl/ssl.key",
        "cert": "/etc/ssl/ssl.crt"
    },
    "http": {
        "port": 80,
        "bind": "0.0.0.0"
    },
    "driver": "redis://redis:6379"
}
